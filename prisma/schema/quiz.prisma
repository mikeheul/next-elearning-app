model Quiz {
    id          String     @id @default(auto()) @map("_id") @db.ObjectId
    title       String
    description String?   

    questions   Question[] 
    
    courseId    String    @db.ObjectId
    course      Course    @relation(fields: [courseId], references: [id])
    
    createdAt   DateTime  @default(now())
    updatedAt   DateTime  @updatedAt
}